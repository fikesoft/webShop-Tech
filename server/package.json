    "prettify": "prettier --config ../.prettierrc.yml --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,jsx,json,scss,css,md}' "
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
